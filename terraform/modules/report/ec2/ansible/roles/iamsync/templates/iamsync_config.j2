iamsync:
  - iam_group: support
    sudo_rule: "ALL= NOPASSWD: /bin/su - postgres"
    local_gid: 1024
  - iam_group: engineering
    sudo_rule: "ALL=(ALL) NOPASSWD:ALL"
    local_gid: 1025
{% if contractor_enabled is defined %}
  - iam_group: contractors
    sudo_rule: "ALL= NOPASSWD: /bin/su - postgres"
    local_gid: 1026
{% endif %}
